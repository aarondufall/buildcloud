<h1>New Project</h1>
 <div class="row">
  <div class="span4 well offset4">

  <%= form_for @project do |f| %>

  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :job_number %>
  <%= f.text_field :job_number %>

  <%= f.submit "Create Project", :class => 'btn btn-primary' %>
  <%= link_to 'Cancel', projects_path, :class => 'btn' %>

<% end %>

 </div>
</div>