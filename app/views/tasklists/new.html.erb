<h1>New TaskList</h1>
 <div class="row">
  <div class="span4 offset4">

  <%= form_for @tasklist, :url => project_tasklists_path do |f| %>

  <%= f.label :name %><br />
  <%= f.text_field :name %>
  <%= f.label :start_date %>
  <%= f.date_select :start_date %>

  <%= f.label :end_date %>
  <%= f.date_select :end_date %>


  <%= f.submit "Create List", :class => 'btn btn-success' %>


  <% end %>

 </div>
</div>